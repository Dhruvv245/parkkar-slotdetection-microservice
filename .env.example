# Environment configuration template
# Copy this file to .env and modify as needed

# Application settings
PYTHONPATH=/app
PYTHONUNBUFFERED=1

# FastAPI settings
HOST=0.0.0.0
PORT=8000
WORKERS=1

# Logging
LOG_LEVEL=info

# Node.js server URL for notifications
NODE_SERVER_URL=https://parkkar.onrender.com/api/v1/parkings/slot-update

# Video processing settings
FPS_LIMIT=25
DETECTION_THRESHOLD=10000

# Health check settings
HEALTH_CHECK_INTERVAL=30s
